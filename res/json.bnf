root ::= object

sep ::= [ \t\n]*
object ::= boolean | null | string | decimal | integer | map | list

string ::= "\"" (escapeSequence | strChar)+ "\""
escapeSequence ::= "\\" [^]
strChar ::= [^"]

integer ::= [0-9]+
decimal ::= [0-9]+ "." [0-9]+
boolean ::= "true" | "false"
null ::= "null"

list ::= "[" sep (object ("," sep object)*)? sep "]"
map ::= "{" sep (mapEntry ("," sep mapEntry)*)? sep "}"
mapEntry ::= string sep ":" sep object